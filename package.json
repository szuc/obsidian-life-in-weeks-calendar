		"@types/moment": "^2.11.29",
		"@types/node": "^24.10.1",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.25.12",
		"esbuild-svelte": "^0.9.3",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"obsidian": "^1.10.3",
		"prettier": "^3.6.2",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@eslint/js": "^9.39.1",
		"date-fns": "^4.1.0",
